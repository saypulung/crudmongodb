 <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Data Tables
        <small>advanced tables</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Tables</a></li>
        <li class="active">Data tables</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Data Orang</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example2" class="table table-bordered table-hover">
                <thead>
                <tr>
                  
                  <th>Nama</th>
                  <th>Alamat</th>
                  <th>Tanggal Lahir</th>
                  <th>Aksi</th>
                </tr>
                </thead>
                <tbody>
                    <% data_orang.forEach(function(orang){ %>
                    <tr>
                        <td><%= orang.nama %></td>
                        <td><%= orang.alamat %></td>
                        <td><%= orang.tanggal_lahir %></td>
                        <td>
                            
                            <a href="/editdata/<%= orang._id %>" class="btn btn-primary btn-xs">Edit</a>
                            <a data-id="<%= orang._id %>" href="javascript:;" class="deldata btn btn-danger btn-xs">Hapus</a>
                        </td>
                    </tr>
                    <%
                    })        
                    %>
                </tbody>
              </table>
            </div>
            
          </div>
        </div>
      </div>
    </section>
 </div>